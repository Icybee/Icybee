		"icybee/module-modules": "*",
		"icybee/module-nodes": "2.x",
		"icybee/module-pages": "2.x",
		"icybee/module-sites": "2.x",
		"icybee/module-users": "2.x",
		"icybee/module-users-roles": "2.x",
		"icybee/module-users-noncelogin": "2.x",
		"icybee/module-thumbnailer": "2.x",
		"icybee/module-views": "2.x",
		"icybee/module-taxonomy-vocabulary": "2.x"

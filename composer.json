{
	"name": "icybee/icybee",
	"type": "library",
	"description": "A buzzing CMS for PHP5.5+",
	"keywords": ["cms"],
	"homepage": "http://icybee.org/",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "http://www.weirdog.com/",
			"role" : "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/Icybee/Icybee/issues",
		"source": "https://github.com/Icybee/Icybee"
	},

	"minimum-stability": "dev",

	"require": {
		"php": ">=5.5.0",
		"icanboogie/icanboogie": "^2.4.0",
		"icanboogie/bind-cldr": "^0.3.0",
		"icanboogie/bind-activerecord": "^0.3.0",
		"icanboogie/bind-facets": "^0.2.0",
		"icanboogie/bind-view": "^0.4.0",
		"icanboogie/i18n": "^2.0.8",
		"icanboogie/module": "^2.3.0",
		"brickrouge/brickrouge": "^2.1.4",
		"brickrouge/css-class-names": "^2.0.0",
		"icybee/core": "^2.1.0",
		"icybee/bluetihi": "^0.0.3",
		"icybee/patron": "^1.4.0",
		"icybee/patron-render-support": "^0.2.0",
		"icybee/module-registry": "^2.1.0",
		"icybee/module-cache": "^2.1.0",
		"icybee/module-dashboard": "^2.1.0",
		"icybee/module-editor": "^2.1.0",
		"icybee/module-forms": "^2.1.0",
		"icybee/module-images": "^2.1.0",
		"icybee/module-modules": "^2.0.0",
		"icybee/module-pages": "^2.1.0",
		"icybee/module-users-roles": "^2.1.0",
		"icybee/module-users-noncelogin": "^2.1.0",
		"icybee/module-views": "^2.1.0"
	},
	"autoload": {
		"classmap": [ "lib" ],
		"psr-4": {
			"Icybee\\": "lib"
		},
		"files": [ "bootstrap.php" ]
	},

	"extra": {
		"icanboogie": {
			"config-path": "config",
			"locale-path": "locale",
			"config-constructor": {
				"admin_routes": "Icybee\\Hooks::synthesize_admin_routes#routes"
			}
		}
	}
}

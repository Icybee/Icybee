{
	"name": "icybee/icybee",
	"type": "library",
	"description": "A buzzing CMS for PHP5.4+",
	"keywords": ["cms"],
	"homepage": "http://icybee.org/",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "http://www.weirdog.com/",
			"role" : "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/Icybee/Icybee/issues",
		"source": "https://github.com/Icybee/Icybee"
	},
	"require": {
		"php": ">=5.4.0",
		"icanboogie/icanboogie": "~2.3.0",
		"icanboogie/i18n": "~2.0.8",
		"icanboogie/cldr": "~1.3.8",
		"icanboogie/module": "~2.2.0",
		"icanboogie/bind-cldr": "~0.2.1",
		"brickrouge/brickrouge": "~2.1.4",
		"brickrouge/css-class-names": "~2.0.0",
		"icybee/core": "2.1.x@dev",
		"icybee/bluetihi": "~0.0.3",
		"icybee/patron": "~1.3.4",
		"icybee/patron-render-support": "~0.1.3",
		"icybee/module-registry": "~2.0",
		"icybee/module-cache": "~2.0",
		"icybee/module-dashboard": "dev-master",
		"icybee/module-editor": "2.x@dev",
		"icybee/module-files": "2.x@dev",
		"icybee/module-forms": "2.x@dev",
		"icybee/module-images": "2.x@dev",
		"icybee/module-modules": "2.x@dev",
		"icybee/module-nodes": "2.x@dev",
		"icybee/module-pages": "2.x@dev",
		"icybee/module-sites": "~2.0",
		"icybee/module-users": "~2.0",
		"icybee/module-users-roles": "2.x@dev",
		"icybee/module-users-noncelogin": "~2.0",
		"icybee/module-views": "2.x@dev"
	},
	"autoload": {
		"classmap": [ "lib/", "includes/" ],
		"files": [ "bootstrap.php" ]
	},

	"extra": {
		"icanboogie": {
			"config-path": "config",
			"locale-path": "locale",
			"config-constructor": {
				"admin_routes": "Icybee\\Hooks::synthesize_admin_routes#routes"
			}
		}
	}
}

{
	"name": "icybee/icybee",
	"type": "library",
	"description": "A buzzing CMS for PHP",
	"keywords": [
		"cms"
	],
	"homepage": "http://icybee.org/",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "http://olvlv.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/Icybee/Icybee/issues",
		"source": "https://github.com/Icybee/Icybee"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"prefer-dist": true,
	"require": {
		"php": ">=5.6",
		"icanboogie/bind-facets": "^0.7",
		"icanboogie/bind-view": "^0.9",
		"brickrouge/brickrouge": "^3.0",
		"brickrouge/css-class-names": "^2.0",
		"brickrouge/listview": "dev-master",
		"icybee/core": "^4.0",
		"icybee/patron-render-support": "^0.3",
		"icybee/module-registry": "^4.0",
		"icybee/module-cache": "^4.0",
		"icybee/module-dashboard": "^4.0",
		"icybee/module-editor": "^4.0",
		"icybee/module-forms": "^4.0",
		"icybee/module-images": "^4.0",
		"icybee/module-modules": "^4.0",
		"icybee/module-pages": "^4.0",
		"icybee/module-users-roles": "^4.0",
		"icybee/module-users-noncelogin": "^4.0",
		"icybee/module-views": "^4.0"
	},
	"autoload": {
		"classmap": [
			"lib"
		],
		"psr-4": {
			"Icybee\\": "lib"
		},
		"files": [
			"bootstrap.php"
		]
	},
	"extra": {
		"icanboogie": {
			"config-path": "config",
			"locale-path": "locale",
			"config-constructor": {
				"admin_routes": "Icybee\\Hooks::synthesize_admin_routes#routes"
			}
		}
	}
}
